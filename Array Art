#include <iostream>
using namespace std;

int main()
{
    string snacks[5][5] = {
        {"-", "-", "-", "-", "-"},
        {"-", "0", "-", "0", "-"},
        {"-", "@", "@", "@", "-"},
        {"-", "^", "^", "^", "-"},
        {"-", "V", "V", "V", "-"}
    };

    for (int x = 0; x < 5; x++) {
        for (int y = 0; y < 5; y++) {
            cout << snacks[x][y];
        }
        cout << endl;
    }

    string snack[5][1] = {
        {"-----"},
        {"-0-0-"},
        {"-@@@-"},
        {"-^^^-"},
        {"-VVV-"}
    };

    for (int w = 0; w < 5; w++) {
        for (int z = 0; z < 1; z++) {
            cout << snack[w][z];
        }
        cout << endl;
    }
}
